<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.pesanan.TabBelumBayar">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:paddingRight="@dimen/padding_small_5"
        android:paddingLeft="@dimen/padding_small_5">



        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/recycler_pesanan"
            android:padding="@dimen/padding_small_5"
            tools:listitem="@layout/custom_pesananku"
            android:layout_marginBottom="50dp"
            tools:ignore="NotSibling">

        </androidx.recyclerview.widget.RecyclerView>

        <include layout="@layout/item_data_kosong"/>
    </RelativeLayout>


</FrameLayout>